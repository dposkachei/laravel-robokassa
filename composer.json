{
    "name": "chelout/robokassa",
    "description": "Laravel Robokassa",
    "keywords": [
        "laravel",
        "robokassa"
    ],
    "homepage": "https://github.com/chelout/robokassa",
    "license": "MIT",
    "authors": [
        {
            "name": "Viacheslav Ostrovskiy",
            "email": "chelout@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^7.0",
        "lexty/robokassa": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Chelout\\Robokassa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chelout\\Robokassa\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chelout\\Robokassa\\RobokassaServiceProvider"
            ],
            "aliases": {
                "Robokassa": "Chelout\\Robokassa\\Facade"
            }
        }
    },
    "minimum-stability": "stable"
}
